    "jsonwebtoken": "^8.5.1",
    "knex": "^0.21.4",
    "morgan": "^1.10.0",
    "pg": "^8.3.0",
    "postgrator-cli": "^3.3.0",
    "xss": "^1.0.8"
  },
  "devDependencies": {
    "chai": "^4.2.0",
